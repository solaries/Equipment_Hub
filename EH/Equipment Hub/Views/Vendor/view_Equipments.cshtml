@model IEnumerable<Equipment_hub.Models.Equipment_hub_Equipment_data>
 
 
<div class="card">
    <div class="card-body">
        <h4 class="card-title">View Equipments</h4> 
        <div class="table-responsive">
            <table class="table table-striped table-bordered zero-configuration">
                <thead>
                    <tr>
                        <th>   </th> 
                        <th>Equipment Type</th>
                        <th>Equipment Code</th>
                     
 
                    </tr> 
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>                            
                            <td>@Html.ActionLink("Select", "edit_Equipments", new
                           {
                            id=item.Id, 
                            Vendor = item.Vendor
                            ,Equipment_Code = item.Equipment_code
                            ,Equipment_Type = item.Equipment_type
                               })</td> 
                           <td>@Html.DisplayFor(modelItem => item.Equipment_type_data)</td>
                           <td>@Html.DisplayFor(modelItem => item.Equipment_code)</td>
                            
                        </tr>
                    }
                    
                </tbody>
                <tfoot>
                    <tr>                        
                        <th>   </th> 
                        <th>Equipment Type</th>
                        <th>Equipment Code</th>
                        
 
                    </tr>
                </tfoot>
            </table>
        </div>
    </div>
</div> 
<script> 
    function msg(txt) {
        sweetAlert("...", txt, "info");
    }
    var statusMessage = "@ViewBag.status" || false; 
    if (statusMessage != false && statusMessage.length > 0) {
        msg(statusMessage); 
    }
</script>