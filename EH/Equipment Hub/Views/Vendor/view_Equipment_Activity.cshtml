@model IEnumerable<Equipment_hub.Models.Equipment_hub_Equipment_Activity_data>
 
 
<div class="card">
    <div class="card-body">
        <h4 class="card-title">View Equipment Activity</h4> 
        <div class="table-responsive">
            <table class="table table-striped table-bordered zero-configuration">
                <thead>
                    <tr>
                        <th>   </th> 
                        <th>equipment type</th>
                        <th>Number Of Units</th>
                        <th>Rent Rate</th>
                        <th>Rent Rate Qunatity</th>
                        <th>Customer Company</th>
                        <th>Vendor Company</th>
                        <th>Activity Status</th>
                        <th>activity date</th>
 
                    </tr> 
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>                            
                            <td>@Html.ActionLink("Select", "edit_Equipment_Activity", new
                           {
                            id=item.Id, 
                            activity_date = item.Activity_date
                            ,Activity_Status = item.Activity_status
                            ,Vendor_Company = item.Vendor_company
                            ,Customer_Company = item.Customer_company
                            ,Rent_Rate_Qunatity = item.Rent_rate_qunatity
                            ,Rent_Rate = item.Rent_rate
                            ,Number_Of_Units = item.Number_of_units
                            ,equipment_type = item.Equipment_type
                               })</td> 
                           <td>@Html.DisplayFor(modelItem => item.Equipment_type_data)</td>
                           <td>@Html.DisplayFor(modelItem => item.Number_of_units)</td>
                           <td>@Html.DisplayFor(modelItem => item.Rent_rate_data)</td>
                           <td>@Html.DisplayFor(modelItem => item.Rent_rate_qunatity)</td>
                           <td>@Html.DisplayFor(modelItem => item.Customer_company_data)</td>
                           <td>@Html.DisplayFor(modelItem => item.Vendor_company_data)</td>
                           <td>@Html.DisplayFor(modelItem => item.Activity_status_data)</td>
                           <td>@Html.DisplayFor(modelItem => item.Activity_date)</td>
                        </tr>
                    }
                    
                </tbody>
                <tfoot>
                    <tr>                        
                        <th>   </th> 
                        <th>equipment type</th>
                        <th>Number Of Units</th>
                        <th>Rent Rate</th>
                        <th>Rent Rate Qunatity</th>
                        <th>Customer Company</th>
                        <th>Vendor Company</th>
                        <th>Activity Status</th>
                        <th>activity date</th>
 
                    </tr>
                </tfoot>
            </table>
        </div>
    </div>
</div> 
<script> 
    function msg(txt) {
        sweetAlert("...", txt, "info");
    }
    var statusMessage = "@ViewBag.status" || false; 
    if (statusMessage != false && statusMessage.length > 0) {
        msg(statusMessage); 
    }
</script>